{
	"name": "@project-pokedex/backend",
	"version": "1.0.0",
	"main": "src/server.js",
	"license": "MIT",
	"scripts": {
		"dev": "nodemon",
		"start": "node src/server.js",
		"lint": "prettier --write \"src/**/*.(jsx?|json|less)\" && eslint --fix src/**/*.(jsx?|json)"
	},
	"lint-staged": {
		"src/**/*.(jsx?|json|less)": [
			"prettier --write"
		],
		"src/**/*.(jsx?|json)": [
			"eslint --fix"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"dependencies": {
		"axios": "^1.2.0",
		"celebrate": "^15.0.1",
		"cors": "^2.8.5",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"mongoose": "^6.11.3",
		"pokedex-promise-v2": "^4.0.0"
	},
	"devDependencies": {
		"eslint": "^8.29.0",
		"husky": "^8.0.2",
		"lint-staged": "^13.1.0",
		"nodemon": "^2.0.20"
	}
}
