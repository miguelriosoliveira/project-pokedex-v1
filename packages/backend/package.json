{
	"name": "@project-pokedex/backend",
	"version": "1.0.0",
	"main": "src/server.js",
	"license": "MIT",
	"scripts": {
		"dev": "nodemon",
		"start": "node src/server.js",
		"lint": "prettier --write \"src/**/*.(jsx?|json|less)\" && eslint --fix src/**/*.(jsx?|json)"
	},
	"lint-staged": {
		"src/**/*.(jsx?|json|less)": [
			"prettier --write"
		],
		"src/**/*.(jsx?|json)": [
			"eslint --fix"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"dependencies": {
		"axios": "^0.21.1",
		"celebrate": "^12.0.1",
		"cors": "^2.8.5",
		"dotenv": "^16.0.1",
		"express": "^4.17.1",
		"mongoose": "^5.7.5",
		"pokedex-promise-v2": "^3.2.0"
	},
	"devDependencies": {
		"eslint": "^7.18.0",
		"husky": "^4.2.3",
		"lint-staged": "^10.1.1",
		"nodemon": "^1.19.1"
	}
}
